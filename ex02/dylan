 find . 		ponto para incluir o proprio directory
 -type f 		vai procurar o tipo f ( file )
 -name "*sh"	vai procurar o que acaba com .sh
 cut -c 3/ 		vai cortar os dois primeiros caracteres ( ./ )
 rev 			da reverse as linhas ( abcd -> dcba )
 cut -c 4- 		corta os 3 primeiros caracters ( .sh )
 cut -d '/' -f1	corta tudo a esquerda de /
 rev 			reverse outra vez ( dcba -> abcd)

